{
  "name": "@unsync/veolia-client",
  "type": "module",
  "version": "1.0.3",
  "license": "MIT",
  "repository": "unsync/veolia-client",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "rm -Rf dist && tsc",
    "checkdeps": "ncu",
    "checkdeps:apply": "ncu -u",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky",
    "test": "npm run build && node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "release:patch": "npm version -m 'release:patch' patch",
    "release:minor": "npm version -m 'release:minor' minor",
    "release:major": "npm version -m 'release:major' major"
  },
  "dependencies": {
    "@unsync/nodejs-tools": "^1.0.2",
    "dayjs": "^1.11.10",
    "fast-xml-parser": "^4.3.3"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.6",
    "@unsync/eslint": "^1.0.10",
    "@unsync/tsconfig": "^1.0.9",
    "husky": "^9.0.2",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "npm-check-updates": "^16.14.12",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "*.{js,ts,vue,json}": "eslint --fix ."
  }
}
